{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "HealthChecks": "Debug"
    }
  },
  "AllowedHosts": "*",
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "MyWorld API",
        "Uri": "http://localhost:5001/healthz"
      },
      {
        "Name": "MasterData API",
        "Uri": "http://localhost:5002/healthz"
      }
    ],
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60,
    "MaximumExecutionHistoriesPerEndpoint": 100
  }
}
